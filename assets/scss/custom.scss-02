$color-primary: #d70672;
$color-secondery: #4100f5;
$darkbg: #181818;

img {
  max-width: 100%;
}
body {
  font-size: 16px;
  line-height: 1.5;
  font-weight: 400;
  font-family: 'Poppins', sans-serif;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  small {
    font-weight: inherit;
  }
}
.container-fluid {
  max-width: 95%;
}
#header {
  background: $darkbg;
  .menu-toggler {
    background: none;
    border: none;
    padding: 10px;
    margin-right: -10px;
    margin-left: 14px;
    @media (max-width: 575px) {
      margin-left: 5px;
    }
    span {
      display: block;
      width: 24px;
      height: 2px;
      margin-bottom: 6px;
      background: #fff;
      margin-left: auto;
      &:last-child {
        width: 16px;
        margin-bottom: 0;
      }
    }
  }
  .socials {
    &::before {
      content: '';
      height: 52px;
      border-left: 1px solid rgba(255, 255, 255, 0.27);
      display: inline-block;
      vertical-align: middle;
    }
    a {
      margin-left: 16px;
      display: inline-block;
      @media (max-width: 575px) {
        margin-left: 9px;
      }
    }
  }
}
.socials {
  a {
    display: inline-block;
  }
}
.header-nav {
  .vc {
    display: inline-block;
    vertical-align: middle;
  }
}
.home-section-menu {
  padding: 0;
  margin: 0;
  list-style: none;
  font-size: 18px;
  li {
    display: inline-block;
    margin-right: 24px;
    a {
      display: block;
      position: relative;
      padding: 20px 0;
      color: #fff;
      text-decoration: none;
      &::after {
        content: '';
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 4px;
        background-color: $color-primary;
        transform: scaleX(0);
        transition: all 0.3s;
      }
      &:hover,
      &.active {
        &::after {
          transform: scaleX(1);
        }
      }
    }
  }
}
.menu-overlay {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 110;
}
.open-menu .menu-overlay {
  display: block;
}

.menu-close {
  position: absolute;
  top: 20px;
  right: 20px;
  height: 24px;
  width: 24px;
  cursor: pointer;
  span {
    display: block;
    width: 24px;
    height: 2px;
    background-color: #fff;
    position: absolute;
    left: 0;
    top: 12px;
    &:first-child {
      transform: rotate(45deg);
    }
    &:last-child {
      transform: rotate(-45deg);
    }
  }
}
.menu-wrapper {
  position: fixed;
  top: 0;
  right: -300px;
  height: 100%;
  width: 300px;
  background: #000;
  color: #fff;
  z-index: 120;
  display: flex;
  flex-direction: column;
  padding-left: 29px;
  transition: 0.3s ease-out;
}
.open-menu .menu-wrapper {
  right: 0;
}
.menu-wrapper a {
  display: block;
  color: #fff;
  font-size: 22px;
  text-decoration: none;
}

.menu-wrapper ul {
  list-style: none;
  padding: 0;
  margin: 50px 0px;
  &.home-section-menu li {
    display: block;
    margin: 0;
  }
}

.menu-wrapper ul li {
  margin-bottom: 20px;
}

.banner {
  background-color: #454545;
  .singers {
    padding-top: 37px;
    position: relative;
    z-index: 5;
    max-width: 700px;
    margin: 0 auto;

    ul {
      .flipster__item__content {
        width: 254px;
        height: 308px;
        border-radius: 8px;
        background: rgba(140, 140, 140, 0.87);
        box-shadow: 0 -10px 13px -3px rgba(0, 0, 0, 0.2);
        text-align: center;
        padding-top: 42px;
        @media (max-width: 500px) {
          width: 168px;
          height: 242px;
          padding-top: 26px;
        }
      }
      .dp {
        width: 150px;
        height: 150px;
        border-radius: 50%;
        border: 2px solid #fff;
        background: #000;
        overflow: hidden;
        margin: 0 auto 16px;
        img {
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
        @media (max-width: 500px) {
          width: 124px;
          height: 124px;
          margin: 0 auto 10px;
        }
      }
      p {
        font-size: 18px;
        color: #fff;
        @media (max-width: 500px) {
          font-size: 16px;
        }
      }
    }
    .flipster__button {
      height: 44px;
      width: 44px;
      background: $color-secondery;
      color: #fff;
      border: 2px solid #e2e7fa;
      border-radius: 50%;
      margin: -1em 0px;
      opacity: 1;
      svg {
        width: 14px;
      }
    }
    @media (max-width: 500px) {
      .flipster__button--prev {
        left: 6px;
      }
      .flipster__button--next {
        right: 6px;
      }
    }
  }

  .flipster--carousel .flipster__item--past-1,
  .flipster--carousel .flipster__item--future-1,
  .flipster--carousel .flipster__item--past-2,
  .flipster--carousel .flipster__item--future-2 {
    opacity: 1;
  }

  .equipment {
    position: relative;
    margin-top: -32px;
    text-align: center;
    background: radial-gradient(50% 50% at 50% 50%, #be8a3b 0%, #454545 100%)
      no-repeat center;
    background-size: 1440px auto;

    .img {
      position: relative;
      z-index: 10;
      display: inline-block;
      vertical-align: bottom;
    }
    .layerbg {
      position: absolute;
      width: 100%;
      top: 22%;
      left: 0;
      transform: translate(0, -50%);
      z-index: 0;
      pointer-events: none;
    }
  }
}

.brand {
  font-family: 'Saira Extra Condensed', sans-serif;
  font-weight: 700;
}

section {
  &.gap {
    margin: 105px 0;
  }
}

.header-title {
  margin-bottom: 55px;
  text-align: center;
  .sub {
    font-size: 16px;
    margin-bottom: 0;
    color: $color-secondery;
    &::before {
      display: inline-block;
      content: '';
      border-bottom: 1px solid $color-secondery;
      width: 30px;
      margin-right: 6px;
      vertical-align: middle;
    }
  }
  h2 {
    font-size: 32px;
    color: #000;
  }
}
.row.small-gutter {
  margin-left: -10px;
  margin-right: -10px;
  .col,
  [class^='col-'] {
    padding: 0 10px;
    margin-bottom: 20px;
  }
}
.cat-box {
  background: #000;
  position: relative;
  height: 200px;
  overflow: hidden;
  border-radius: 10px;

  img {
    opacity: 0.7;
    height: 100%;
    width: 100%;
    object-fit: cover;
  }
  h3 {
    position: absolute;
    left: 15px;
    bottom: 5px;
    color: #fff;
    font-size: 20px;
  }
}
#_top-songs {
  background: url(../images/topBg1.svg) no-repeat left top,
    url(../images/topBg2.svg) no-repeat right top;
}
#_categories {
  padding-top: 70px;
  background: url(../images/catBg.svg) no-repeat left top;
}
.user-meta .rounded-circle {
  object-fit: cover;
  width: 160px;
  height: 160px;
}
.more_text {
  p {
    display: -webkit-box;
    line-clamp: 5;
    -webkit-line-clamp: 5;
    -webkit-box-orient: vertical;
    overflow: hidden;
    max-height: 7.5em;
    line-height: 1.5em;
    transition: max-height 0.5s ease-in-out;
    &.clamped {
      line-clamp: initial;
      -webkit-line-clamp: initial;
    }
  }
  &.expanded p {
    max-height: 40em;
  }
  .more_btn {
    color: #4100f5;
    font-size: 14px;
    position: relative;
    top: -12px;
    cursor: pointer;
  }
}
.top-item {
  position: relative;
  padding-bottom: 100%;
  a {
    color: inherit;
    text-decoration: none;
  }
  > img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    position: absolute;
  }
  .caption {
    background: rgba(0, 0, 0, 0.44);
    position: absolute;
    left: 0;
    bottom: 20px;
    padding: 9px 12px;
    max-width: 100%;
    h3 {
      margin: 0;
      font-size: 18px;
      color: #fff;
      white-space: nowrap;
      text-overflow: ellipsis;
      overflow: hidden;
    }
  }
}
.top-more {
  height: 100%;
  display: flex;
  background: linear-gradient(100.92deg, #4100f5 1.37%, #7847ff 81.91%);
  color: #fff;
  border-radius: 8px;
  padding: 14px 10px;
  h3 {
    margin: auto;
    font-size: 20px;
    img {
      margin-left: 12px;
    }
  }
}
.side-banner {
  .bg {
    width: 100%;
    margin-bottom: 20px;
  }
}
#_intro {
  color: #555555;
  font-size: 15px;
  h3,
  h4 {
    color: #000;
  }

  .point {
    padding-left: 55px;
    background: transparent url(../images/tick.svg) no-repeat left top;
    h4 {
      font-size: 18px;
    }
  }
}

#footer {
  background: $darkbg;
  padding-top: 48px;
  color: #fff;
}

.inner-cover-bg {
  background-position: 0 50%;
  background-size: 100% auto;
}
.twPc-block {
  display: block !important;
}
.left-inner {
  z-index: 4 !important;
  position: relative;
  top: -120px;
  border-radius: 16px;
}
.profile-bg {
  background: #fff;
  color: #5c5c5c;
  padding: 26px 24px;
  font-size: 15px;
  z-index: 15;
  box-shadow: 10px 8px 59px -25px rgba(0, 0, 0, 0.71);
}
.profile-img {
  width: 100px;
  height: 100px;
  line-height: 50px;
  background: #000;
  text-align: center;
  display: block;
  margin: 0 auto;
  border-radius: 50px;
}
.profile-bg h4 {
  font-size: 22px;
  margin: 18px 0 10px;
  color: #000;
}
.profile-bg span {
  text-align: center;
  margin-top: 15px;
}
.btns {
  font-size: 0;
  margin: 0 -5px;
  a {
    margin: 5px;
  }
}
a.follow-btn {
  padding: 8px;
  border-radius: 5px;
  background-color: #4100f5;
  color: #fff;
  font-size: 15px;
  line-height: 24px;
  font-weight: 500;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  min-width: 150px;
  min-height: 40px;
  vertical-align: middle;
  transition: 0.3s;
  svg {
    margin-right: 4px;
    path {
      fill: currentColor;
    }
  }
  &:hover {
    box-shadow: 0 11px 18px -10px rgba(0, 0, 0, 0.7);
  }
  &.opp {
    background-color: #ddd4f6;
    color: #4100f5;
    &:hover {
      background-color: #4100f5;
      color: #fff;
    }
  }
}
.right-inner {
  padding: 80px 35px;
}
.m_top_30 {
  margin-top: 30px;
}
.m_bottom_30 {
  margin-bottom: 30px;
}
.own_profile_text h4 {
  color: #222;
  font-size: 16px;
  font-weight: 500;
  margin-bottom: 8px;
  padding-bottom: 10px;
  position: relative;
  text-transform: capitalize;
}
.own_profile_text h4::before {
  background: #686868 none repeat scroll 0 0;
  bottom: 20px;
  content: '';
  height: 1px;
  position: absolute;
  width: 35px;
  left: 0;
}
.own_profile_text h4::after {
  background: #686868 none repeat scroll 0 0;
  bottom: 20px;
  content: '';
  height: 1px;
  position: absolute;
  width: 35px;
  right: 0;
}
.own_profile_text p {
  text-align: left;
}
.own_profile_text a {
  text-align: center !important;
  padding: 0 !important;
  color: #878787;
}
#footer h5 {
  margin-bottom: 20px;
  color: #fff;
}
#footer ul.quick-links li {
  padding: 3px 0;
  -webkit-transition: 0.5s all ease;
  -moz-transition: 0.5s all ease;
  transition: 0.5s all ease;
}
#footer ul.quick-links li:hover {
  padding: 3px 0;
  margin-left: 5px;
  font-weight: 700;
  text-decoration: none;
  text-align: left !important;
}
#footer ul.quick-links li a {
  color: #efefef;
  text-decoration: none;
  text-align: left !important;
}
#footer ul.quick-links li a i {
  margin-right: 5px;
}
#footer ul.quick-links li:hover a i {
  font-weight: 700;
}
.bottom-nav {
  padding: 10px 0;
  border-top: 1px solid #5d5d5d;
  border-bottom: 1px solid #5d5d5d;
  width: 100%;
  display: block;
}
.footer-nav ul {
  padding: 0;
  margin: 0 -13px;
  list-style: none;
  li {
    display: inline-block;
    a {
      padding: 0 13px;
      text-decoration: none;
      display: block;
      color: #d0d0d0;
    }
  }
}
.copyright {
  color: #8d8d8d;
  font-size: 14px;
}
.copyright a {
  font-weight: 500;
  color: #a5a5a5;
}

.social {
  padding: 0;
  margin: 0;
  img {
    width: 40px;
  }
}
.onview {
  opacity: 0;
  transform: translateY(30px);
  &.viewed {
    transition: 0.3s ease;
    opacity: 1;
    transform: translateY(0px);
  }
}
#menu__toggle {
  opacity: 0;
}
#menu__toggle:checked ~ .menu__btn > span {
  transform: rotate(45deg);
}
#menu__toggle:checked ~ .menu__btn > span::before {
  top: 0;
  transform: rotate(0);
}
#menu__toggle:checked ~ .menu__btn > span::after {
  top: 0;
  transform: rotate(90deg);
}
#menu__toggle:checked ~ .menu__box {
  visibility: visible;
  right: 0;
}
.menu__btn {
  display: flex;
  align-items: center;
  position: absolute;
  top: 20px;
  right: 0px;
  width: 26px;
  height: 26px;
  cursor: pointer;
  z-index: 8;
}
.menu__btn > span,
.menu__btn > span::after,
.menu__btn > span::before {
  display: block;
  position: absolute;
  width: 100%;
  height: 2px;
  background-color: #ffffff;
  transition-duration: 0.25s;
}
.menu__btn > span::before {
  content: '';
  top: -8px;
}
.menu__btn > span::after {
  content: '';
  top: 8px;
  width: 16px;
  right: 0px;
}
.menu__box {
  display: block;
  position: fixed;
  visibility: hidden;
  top: 0;
  right: -100%;
  width: 300px;
  height: 100%;
  margin: 0;
  padding: 80px 0;
  list-style: none;
  background-color: #eceff1;
  box-shadow: 1px 0 6px rgba(0, 0, 0, 0.2);
  z-index: 5;
  transition-duration: 0.25s;
}
.menu__item {
  display: block;
  padding: 12px 24px;
  color: #333;
  font-family: Roboto, sans-serif;
  font-size: 20px;
  font-weight: 600;
  text-decoration: none;
  transition-duration: 0.25s;
}
.menu__item:hover {
  background-color: #cfd8dc;
}
@media only screen and (min-width: 992px) and (max-width: 1199px) {
  .w-button {
    margin-top: 10px !important;
  }
  .home-section-menu li a {
    padding: 13px 0;
  }
}
@media only screen and (min-width: 481px) and (max-width: 767px) {
  .left-inner {
    top: -32px;
  }
  .home-section-menu li {
    display: inline-block;
    margin-right: 16px;
  }
  .home-section-menu li a {
    font-size: 10px;
  }
}
@media only screen and (min-width: 320px) and (max-width: 480px) {
  .left-inner {
    top: -15px;
  }
  .footer-nav ul li a {
    font-size: 12px;
    padding: 0 10px;
  }
  .social {
    padding: 0;
    margin: 0 auto;
    display: block;
    text-align: center;
  }
}

.overlay {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  transition: opacity 500ms;
  visibility: hidden;
  opacity: 0;
  z-index: 999;
}
.overlay:target {
  visibility: visible;
  opacity: 1;
}

.popup {
  margin: 70px auto;
  padding: 20px;
  background: #fff;
  border-radius: 5px;
  width: 30%;
  position: relative;
  transition: all 5s ease-in-out;
}

.popup h2 {
  margin-top: 0;
  color: #333;
  font-family: Tahoma, Arial, sans-serif;
}
.popup .close {
  position: absolute;
  top: 20px;
  right: 30px;
  transition: all 200ms;
  font-size: 30px;
  font-weight: light;
  text-decoration: none;
  color: rgb(0, 0, 0);
}
// .popup .close:hover {
//   color: #000000;
// }
.popup .content {
  max-height: 30%;
  overflow: auto;
}

@media screen and (max-width: 700px) {
  .box {
    width: 70%;
  }
  .popup {
    width: 70%;
  }
}

