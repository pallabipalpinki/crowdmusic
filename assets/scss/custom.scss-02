  &.added i {
    font-weight: 900;
  }
}
.like_track {
  border: none;
  background: none;
  position: absolute;
  top: 0;
  right: 15px;
  font-size: 18px;
  padding: 5px;
  line-height: 1;
  height: 30px;
  width: 30px;
  color: #d70672 !important;
  z-index: 2;
  outline: none;
  &:after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(215, 6, 114, 0.1);
    z-index: -1;
    border-radius: 50%;
    transform: scale(0);
    opacity: 0;
  }
  &.liked {
    &::after {
      animation: clickblow 0.4s;
    }
    i {
      color: #d70672;
      font-weight: 900;
    }
  }
}
@keyframes clickblow {
  0% {
    opacity: 0;
    transform: scale(0);
  }
  80% {
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: scale(2);
  }
}
.player {
  padding-bottom: 100%;
  background: $darkbg;
  position: relative;
  cursor: pointer;
  color: #fff;
  overflow: hidden;
}
.player > img {
  height: 100%;
  width: 100%;
  object-fit: cover;
  position: absolute;
  top: 0;
  left: 0;
}
.player::before {
  content: '\f04b';
  font-family: 'Font Awesome 5 Free';
  font-weight: 900;
  color: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 56px;
  opacity: 0;
  z-index: 2;
}
.player::after {
  position: absolute;
  content: '';
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.35);
  z-index: 0;
  left: 0;
  top: 0;
  opacity: 0;
  backdrop-filter: blur(2px);
}
.player audio {
  opacity: 0;
  visibility: hidden;
}
.player canvas {
  width: 100%;
  height: 50%;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
  display: none;
  z-index: 4;
}

.player:hover::before,
.player:hover::after,
.player.playing::after {
  opacity: 1;
}
.player.playing::before {
  opacity: 0;
}
.player.playing canvas {
  display: block;
}
.player .caption {
  z-index: 5;
}
.player:hover .caption,
.player.playing .caption {
  background: transparent;
  max-width: none;
  animation: marquee 3s linear infinite;
}
