#loguserform {
  .form-control {
    border: none;
    background: #f0ebff;
    border-radius: 24px;
    height: 46px;
    padding: 11px 20px;
  }
  .form-group {
    margin-bottom: 1.2rem;
  }
}

.log-btn {
  padding: 17px 23px;
  border-radius: 7px;
  background-color: #4100f5;
  border-color: #4100f5;
  color: #fff;
